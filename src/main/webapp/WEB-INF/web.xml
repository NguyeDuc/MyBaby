<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
 
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>MakeBaby</servlet-name>
  	<display-name>MakeBaby</display-name>
  	<description></description>
  	<servlet-class>controller.MakeBaby</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>LoadImage</servlet-name>
  	<display-name>LoadImage</display-name>
  	<description></description>
  	<servlet-class>controller.LoadImage</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>MakeBaby</servlet-name>
  	<url-pattern>/MakeBaby</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>LoadImage</servlet-name>
  	<url-pattern>/LoadImage</url-pattern>
  </servlet-mapping>
  
  <welcome-file-list>
		<welcome-file>/views/makeBaby.jsp</welcome-file>
  </welcome-file-list>
</web-app>